<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<!-- mbr_coin_chged: 会员积分变更表-->
<mapper namespace="MbrCoinChged">
    <resultMap id="result_base" type="MbrCoinChged">
         	<id property="id" column="id" jdbcType="VARCHAR"/>
         	<result property="memberId" column="member_id" jdbcType="VARCHAR" />
         	<result property="chgType" column="chg_type" jdbcType="INTEGER" />
         	<result property="coin" column="coin" jdbcType="DECIMAL" />
         	<result property="remark" column="remark" jdbcType="VARCHAR" />
         	<result property="crtime" column="crtime" jdbcType="TIMESTAMP" />
    </resultMap>


	<sql id="columns">
	     id,  member_id,  chg_type,  coin,  remark,  crtime 
	</sql>

    <sql id="sql_query_where">
		<where>
            <if test="@Ognl@isNotEmpty(id)"> AND id  = #{id} </if>
            <if test="@Ognl@isNotEmpty(memberId)"> AND member_id  = #{memberId} </if>
            <if test="@Ognl@isNotEmpty(chgType)"> AND chg_type  = #{chgType} </if>
            <if test="@Ognl@isNotEmpty(coin)"> AND coin  = #{coin} </if>
            <if test="@Ognl@isNotEmpty(remark)"> AND remark  = #{remark} </if>
            <if test="@Ognl@isNotEmpty(crtime)"> AND crtime  = #{crtime} </if>
        </where>
    </sql>
    
    <insert id="insert" parameterType="MbrCoinChged">
        insert into mbr_coin_chged(
         <include refid="columns"/>
        ) values(
	    #{id,jdbcType=VARCHAR },#{memberId,jdbcType=VARCHAR },#{chgType,jdbcType=INTEGER },#{coin,jdbcType=DECIMAL },#{remark,jdbcType=VARCHAR },#{crtime,jdbcType=TIMESTAMP }
        )
    </insert>

    <update id="update" parameterType="MbrCoinChged">
        update mbr_coin_chged 
        <set>
           <if test="@Ognl@isNotEmpty(memberId)" >
            member_id= #{memberId,jdbcType=VARCHAR },
           </if>
           <if test="@Ognl@isNotEmpty(chgType)" >
            chg_type= #{chgType,jdbcType=INTEGER },
           </if>
           <if test="@Ognl@isNotEmpty(coin)" >
            coin= #{coin,jdbcType=DECIMAL },
           </if>
           <if test="@Ognl@isNotEmpty(remark)" >
            remark= #{remark,jdbcType=VARCHAR },
           </if>
           <if test="@Ognl@isNotEmpty(crtime)" >
            crtime= #{crtime,jdbcType=TIMESTAMP },
           </if>
       </set>
          where id =#{id}
    </update>

    <delete id="delete" parameterType="java.lang.String">
        delete from mbr_coin_chged 
          where id =#{id}
    </delete>

    <select id="load" parameterType="java.lang.String" resultMap="result_base">
        select * from mbr_coin_chged 
	          where id =#{id}
    </select>

    <select id="query" parameterType="map" resultMap="result_base">
        select * from mbr_coin_chged
        <include refid="sql_query_where"/>
		<if test="@Ognl@isNotEmpty(orderField)">
		order by ${orderField} ${orderSeq}
		</if>
		<if test="@Ognl@isEmpty(orderField)">
		order by crtime desc
		</if>
    </select>

    
    <select id="count"  parameterType="Map" resultType="Integer">
        select count(1) from mbr_coin_chged
        <include refid="sql_query_where"/>
    </select>
</mapper>